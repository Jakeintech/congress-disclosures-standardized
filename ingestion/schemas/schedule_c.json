{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Schedule C",
    "type": "object",
    "description": "Earned income and compensation.",
    "properties": {
        "income": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "source": {
                        "type": "string"
                    },
                    "amount": {
                        "type": "number"
                    },
                    "currency": {
                        "type": "string"
                    }
                },
                "required": [
                    "source",
                    "amount"
                ]
            }
        }
    },
    "required": [
        "income"
    ]
}