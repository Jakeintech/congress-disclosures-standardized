{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Schedule E",
    "type": "object",
    "description": "Positions held (e.g., officer, director).",
    "properties": {
        "positions": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "organization": {
                        "type": "string"
                    },
                    "start_date": {
                        "type": "string",
                        "format": "date"
                    },
                    "end_date": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "format": "date"
                    }
                },
                "required": [
                    "title",
                    "organization",
                    "start_date"
                ]
            }
        }
    },
    "required": [
        "positions"
    ]
}