{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Schedule A",
  "type": "object",
  "description": "Assets owned at the time of filing.",
  "properties": {
    "assets": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {"type": "string"},
          "description": {"type": "string"},
          "value": {"type": "number"},
          "currency": {"type": "string"}
        },
        "required": ["type", "value"]
      }
    }
  },
  "required": ["assets"]
}
