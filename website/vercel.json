{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "ignoreBuildStep": "[[ \"$VERCEL_GIT_COMMIT_REF\" != \"main\" ]]",
  "env": {
    "NEXT_PUBLIC_API_BASE": "https://yvpi88rhwl.execute-api.us-east-1.amazonaws.com"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/website/(.*)",
      "destination": "/$1",
      "permanent": true
    }
  ]
}
