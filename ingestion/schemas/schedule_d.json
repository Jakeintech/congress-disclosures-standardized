{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Schedule D",
    "type": "object",
    "description": "Liabilities and debts.",
    "properties": {
        "liabilities": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string"
                    },
                    "amount": {
                        "type": "number"
                    },
                    "currency": {
                        "type": "string"
                    }
                },
                "required": [
                    "type",
                    "amount"
                ]
            }
        }
    },
    "required": [
        "liabilities"
    ]
}